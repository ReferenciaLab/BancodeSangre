var
  DonacionId: String;
  PacienteId: String;

begin
  inherited;
  
  PacienteId:= DMB.BuscarPaciente;

  if (pacienteId <> '') then
    begin
      DonacionId:= DMB.NuevaDonacion(PacienteId);
    end;

end;

